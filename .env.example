# Variáveis do n8n
N8N_USER=admin
N8N_PASSWORD=n8n_secure_password_abc!
N8N_USER_API_KEY=sua_chave_api_do_n8n_aqui # Gerar em Settings -> My Profile -> API Keys no n8n
